  "bugs": {
    "url": "https://github.com/ZZITE/type-generator-faker-mcp/issues"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.0",
    "@modelcontextprotocol/sdk": "^0.4.0",
    "commander": "^14.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0"
